		Public Function GetString(ByVal culture As String, ByVal key As String) As String
			Dim localization As Dictionary(Of String, String) = Nothing
			Dim value As String = Nothing
			If localizations.TryGetValue(culture, localization) Then
				localization.TryGetValue(key, value)
			End If
			Return value
		End Function
